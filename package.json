{
  "name": "ansker-back",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all build start",
    "start": "nodemon --exec 'babel-node -r ./node_modules/dotenv/config --inspect=0.0.0.0 src/bin/www.js'",
    "prod": "NODE_ENV=production npm-run-all build startProd",
    "startProd": "node dist/bin/www.js",
    "build": "npm-run-all clean transpile",
    "clean": "rm -rf dist",
    "transpile": "babel src --out-dir dist",
    "test": "jest"
  },
  "dependencies": {
    "all-the-cities-mongodb": "^1.0.1",
    "await-to-js": "^2.1.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "country-data": "0.0.31",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "~4.16.0",
    "express-async-handler": "^1.1.4",
    "express-http-proxy": "^1.5.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "mongoose-sequence": "^5.2.2",
    "morgan": "~1.9.0",
    "nodemailer": "^6.4.8",
    "nodemon": "^2.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "query-string": "^6.11.1",
    "socket.io": "^2.2.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "npm-run-all": "^4.1.5",
    "random-username-generator": "^1.0.4",
    "rimraf": "^2.6.3"
  }
}
